{
  "version": 2,
  "functions": {
    "api/index.js": {
      "includeFiles": "dist/ssr/**"
    }
  },
  "rewrites": [
    {
      "source": "/_quasar/(.*)",
      "destination": "/dist/ssr/client/_quasar/$1"
    },
    {
      "source": "/(js|css|fonts|icons|sw.js|favicon.ico)/(.*)",
      "destination": "/dist/ssr/client/$1/$2"
    },
    {
      "source": "/(.*)",
      "destination": "/api/server.js"
    }
  ]
}